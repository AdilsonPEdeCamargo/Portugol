algoritmo "semnome"

var
cotDolar, vlrDolar, qtdReal: real

inicio
escreval("Digite o valor da cotação do Dolar")

leia(cotDolar)

escreval("Digite o a quantidade de reais que você deseja transformar em dolar")
leia(qtdReal)

vlrDolar <-  qtdReal / cotDolar
Escreval  (vlrDolar)
Escreval  ("SUCESSO")






fimalgoritmo
