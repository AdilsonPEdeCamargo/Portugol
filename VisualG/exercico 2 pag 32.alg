algoritmo "semnome"



var

varA: real
varB: real
numero: real


inicio
Escreval ("Digite um número")
leia (numero)

se (numero>=0) entao
   varA <- numero
   Escreval (" O número ", numero , " é positivo ")
senao varB <- numero
   Escreval (" O número ", numero , " é negativo ")
fimse


fimalgoritmo
